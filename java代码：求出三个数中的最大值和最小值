import java.util.Scanner;
public class Test{
 public static void main(String[] args){
  Scanner input=new Scanner(System.in);
  System.out.print("Enter three numbers:");
   int a=input.nextInt();
   int b=input.nextInt();
   int c=input.nextInt();
   int max=0;
   int min=0;
   if(a>b){
	   max=a;
   min=b;}
   else{
	   max=b;
   min=a;}
   if(c>max){
   max=c;}
   else{
	   if(c<min){
	   min=c;}
   }	   
   System.out.println("max="+max);
    System.out.println("min="+min);
   }
 }
 
  